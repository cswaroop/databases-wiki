 <querysource>
  <name>detail</name>
  <sql>SELECT bankadj_id, (bankaccnt_name || '-' || bankaccnt_descrip) AS f_bank,
       bankadjtype_name, formatDate(bankadj_date) AS f_date, bankadj_docnumber,
       formatMoney(bankadj_amount) AS f_amount
  FROM bankadj LEFT OUTER JOIN bankaccnt ON (bankadj_bankaccnt_id=bankaccnt_id)
               LEFT OUTER JOIN bankadjtype ON (bankadj_bankadjtype_id=bankadjtype_id)
 WHERE ( (NOT bankadj_posted)
   AND   (bankadj_bankaccnt_id=&lt;? value(&quot;bankaccnt_id&quot;) ?>) );</sql>
 </querysource>
