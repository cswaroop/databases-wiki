 <querysource>
  <name>detail</name>
  <sql>select usr_username,
       usr_propername,
       usr_initials,
       formatBoolYN(usr_active) AS f_active,
       formatBoolYN(usr_agent) AS f_pa,
       locale_code
  FROM usr, locale
 WHERE ((usr_locale_id=locale_id)
&lt;? if not exists(&quot;showInactive&quot;) ?>
   AND (usr_active=true)
&lt;? endif ?>
)
ORDER BY usr_username;</sql>
