 <querysource>
  <name>detail</name>
  <sql>SELECT formatDate(period_start) AS f_firstperiod,
       formatDate(period_end) AS f_lastperiod,
       formatGLAccountLong(budgitem_accnt_id) AS f_accnt,
       formatMoney(budgitem_amount) AS f_amount
  FROM budgitem 
  JOIN period ON (budgitem_period_id=period_id)
 WHERE(budgitem_budghead_id=&lt;? value("budghead_id") ?>)
 ORDER BY period_start, f_accnt;</sql>
 </querysource>
