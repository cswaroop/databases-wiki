 <title>Print1096</title>
 <name>1096Form</name>
 <description>A report template for printing vendor 1096s onto a pre-printed  form.</description>
 <parameter default="1/1/11" active="true" listtype="static" type="string" name="fromdate">
  <description></description>
 </parameter>
 <parameter default="12/31/11" active="true" listtype="static" type="string" name="todate">
  <description></description>
