 <rightmargin>100</rightmargin>
 <leftmargin>75</leftmargin>
 <querysource loadFromDb="true">
  <name>detail</name>
  <mqlgroup>1096</mqlgroup>
  <mqlname>summary</mqlname>
 </querysource>
 <querysource>
  <name>payer</name>
  <sql>

SELECT 
  taxauth_code, 
  taxauth.taxauth_extref as ein, 
  formataddr(taxauth_addr_id) as paddr,
cntct_name, cntct_phone, cntct_fax, cntct_email
FROM 
  taxauth 
left join crmacct on taxauth_id = crmacct_taxauth_id
left join cntct on crmacct_cntct_id_1 = cntct_id
WHERE 
  taxauth_code = '1099';
</sql>
 </querysource>
 <querysource>
  <name>ein</name>
  <sql>SELECT metric_value AS ein FROM metric WHERE metric_name ='ACHCompanyId';</sql>
 </querysource>
