 <querysource>
  <name>head</name>
  <sql>SELECT vend_number, vend_name
  FROM vendinfo
 WHERE (vend_id=&lt;? value(&quot;vend_id&quot;) ?>);
    </sql>
 </querysource>
 <querysource>
  <name>detail</name>
  <sql>SELECT vendaddr_code as num,
       vendaddr_name as name,
       addr_line1 AS address,
       (addr_city || ', ' || addr_state || '  ' || addr_postalcode)  AS citystatezip
  FROM vendaddrinfo
  LEFT OUTER JOIN addr ON (vendaddr_addr_id=addr_id)
 WHERE (vendaddr_vend_id=&lt;? value(&quot;vend_id&quot;) ?>) 
ORDER BY vendaddr_code;</sql>
 </querysource>
