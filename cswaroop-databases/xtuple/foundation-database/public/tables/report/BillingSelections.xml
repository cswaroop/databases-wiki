 <querysource>
  <name>detail</name>
  <sql>SELECT cobmisc_id,
       cohead_id,
       COALESCE(TEXT(cobmisc_invcnumber), '?') AS docnumber,
       cohead_number,
       cust_number,
       cust_name
  FROM cobmisc, cohead, custinfo
 WHERE ((cobmisc_cohead_id=cohead_id)
   AND (cohead_cust_id=cust_id)
   AND (NOT cobmisc_posted) )
ORDER BY docnumber;</sql>
