SELECT cntctrestore(cntctmrgd_cntct_id) AS result
FROM cntctmrgd
<? if exists("cntct_id") ?>
WHERE (cntctmrgd_cntct_id=<? value("cntct_id") ?>)
<? endif ?>
;
